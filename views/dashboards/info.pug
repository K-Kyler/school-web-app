    a(class="mb-2 px-3 py-2 dashboard__sideMenuFunction dashboard__sideMenuFunction--active", href="/dashboard/info") Info 

    if user.type === "Admin"
        a(class="mb-2 px-3 py-2 dashboard__sideMenuFunction", href="/dashboard/users") Users

    a(class="px-3 py-2 dashboard__sideMenuFunction", href="javascript:void", id="logoutButton") Log out

block dashboardContent
